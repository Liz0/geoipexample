<?xml version="1.0" encoding="utf-8"?>
<!doctype html>
<html>
<head>
  <title>GeoIP</title>
  <style>
  dl {
    border: 3px double #ccc;
    padding: 0.5em;
  }
  dt {
    float: left;
    clear: left;
    width: 100px;
    text-align: right;
    font-weight: bold;
    color: green;
  }
  dt:after {
    content: ":";
  }
  dd {
    margin: 0 0 0 110px;
    padding: 0 0 0.5em 0;
  }
</style>
</head>
<body>
	<dl>
		{% for key, value in data %}
			<dt>{{ key }}</dt><dd>{% if value is empty %}&nbsp;{% else %}{{ value }}{% endif %}</dd>
		{% endfor %}
    </dl>
</body>
</html>
